        $("#bathrooms-information-bathroom-3-column").css("display", "none");
        $("#bathrooms-information-bathroom-4-column").css("display", "none");
        $("#bathrooms-information-bathroom-5-column").css("display", "none");
        //$("#bathrooms-information-guest-bathroom-column").css("display", "none");


        var max = $("#bathrooms-number-of-bathrooms").attr('max');
        if ($("#bathrooms-number-of-bathrooms").val() > max) {
            $("#bathrooms-number-of-bathrooms").val(max);
        }
        if ($("#bathrooms-number-of-bathrooms").val() == "1") {
            $("#bathrooms-information-main-column").css("display", "block");
        } else if ($("#bathrooms-number-of-bathrooms").val() == "2") {
            $("#bathrooms-information-main-column").css("display", "block");
            $("#bathrooms-information-bathroom-2-column").css("display", "block");
        } else if ($("#bathrooms-number-of-bathrooms").val() == "3") {
            $("#bathrooms-information-main-column").css("display", "block");
            $("#bathrooms-information-bathroom-2-column").css("display", "block");
            $("#bathrooms-information-bathroom-3-column").css("display", "block");
        } else if ($("#bathrooms-number-of-bathrooms").val() == "4") {
            $("#bathrooms-information-main-column").css("display", "block");
            $("#bathrooms-information-bathroom-2-column").css("display", "block");
            $("#bathrooms-information-bathroom-3-column").css("display", "block");
            $("#bathrooms-information-bathroom-4-column").css("display", "block");
