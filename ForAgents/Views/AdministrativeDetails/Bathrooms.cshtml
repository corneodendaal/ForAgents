        var numberOfBathrooms = $("#bathrooms-number-of-bathrooms").val();
        localStorage.setItem("numBathrooms", numberOfBathrooms);
        $("#listing-information-bathrooms-input").val(numberOfBathrooms);
    });
    $('#bathrooms-guest-bathroom-check').change(function () {
        var showGuestBathroom = $('input[name="bathrooms-guest-bathroom-check"]:checked').length > 0;
